{
    "expo": {
        "name": "KindCoins",
        "slug": "kindcoins",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/logo/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./src/assets/logo/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/logo/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "favicon": "./src/assets/logo/favicon.png"
        }
    }
}